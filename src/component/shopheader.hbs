{{#with data}}
  <section class='mx-auto'>
    <div class='card'>
      <ul
        class='nav justify-content-between align-items-center'
      >
        <li class='nav-item'>
          <h1 class='nav-brand ms-2'>{{Logo}}</h1>
        </li>
        <li class='nav-item'>
          <ul class='nav'>
            {{#each links}}
              <a
                href='{{href}}'
                class='nav-link'
              >{{text}}</a>
            {{/each}}
          </ul>
        </li>

      </ul>
    </div>

  </section>
{{/with}}